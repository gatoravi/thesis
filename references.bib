@article{suterdm_naeff:MammalianGenes2011a,
  title = {Mammalian {{Genes Are Transcribed}} with {{Widely Different Bursting Kinetics}}},
  author = {Suter, David M. and Molina, Nacho and Gatfield, David and Schneider, Kim and Schibler, Ueli and Naef, Felix},
