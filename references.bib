@article{kfouryn_rubinjb:Brd4boundEnhancers2020,
  title = {Brd4-Bound Enhancers Drive Cell Intrinsic Sex Differences in Glioblastoma},
  author = {Kfoury, Najla and Qi, Zongtai and Prager, Briana C. and Wilkinson, Michael N. and Broestl, Lauren and Berrett, Kristopher C. and Moudgil, Arnav and Sankararaman, Sumithra and Chen, Xuhua and Gertz, Jay and Rich, Jeremy Naftali and Mitra, Robi D. and Rubin, Joshua B.},
  year = {2020},
  month = nov,
  journal = {bioRxiv},
  pages = {199059},
  publisher = {{Cold Spring Harbor Laboratory}},
  doi = {10.1101/199059},
  abstract = {{$<$}p{$>$}Sex can be an important determinant of cancer phenotype, and exploring sex-biased tumor biology holds promise for identifying novel therapeutic targets and new approaches to cancer treatment. In an established isogenic murine model of glioblastoma, we discovered correlated transcriptome-wide sex differences in gene expression, H3K27ac marks, large Brd4-bound enhancer usage, and Brd4 localization to Myc and p53 genomic binding sites. These sex-biased gene expression patterns were also evident in human glioblastoma stem cells (GSCs). These observations led us to hypothesize that Brd4-bound enhancers might underlie sex differences in stem cell function and tumorigenicity in GBM. We found that male and female GBM cells exhibited opposing responses to pharmacological or genetic inhibition of Brd4. Brd4 knockdown or pharmacologic inhibition decreased male GBM cell clonogenicity and in vivo tumorigenesis, while increasing both in female GBM cells. These results were validated in male and female patient-derived GBM cell lines. Furthermore, analysis of the Cancer Therapeutic Response Portal of human GBM samples segregated by sex revealed that male GBM cells are significantly more sensitive to BET inhibitors than are female cells. Thus, for the first time, Brd4 activity is revealed to drive a sex differences in stem cell and tumorigenic phenotype, resulting in diametrically opposite responses to BET inhibition in male and female GBM cells. This has important implications for the clinical evaluation and use of BET inhibitors.{$<$}/p{$>$}},
  chapter = {New Results},
  copyright = {\textcopyright{} 2020, Posted by Cold Spring Harbor Laboratory. The copyright holder for this pre-print is the author. All rights reserved. The material may not be redistributed, re-used or adapted without the author's permission.},
  langid = {english},
  file = {C\:\\Users\\Rilakkuma\\Zotero\\storage\\JRDY5G34\\Kfoury et al. - 2020 - Brd4-bound enhancers drive cell intrinsic sex diff.pdf}
}

@article{kharchenkopv_parkpj:ComprehensiveAnalysis2011,
  title = {Comprehensive Analysis of the Chromatin Landscape in {{Drosophila}}},
  author = {Kharchenko, Peter V. and Alekseyenko, Artyom A. and Schwartz, Yuri B. and Minoda, Aki and Riddle, Nicole C. and Ernst, Jason and Sabo, Peter J. and Larschan, Erica and Gorchakov, Andrey A. and Gu, Tingting and {Linder-Basso}, Daniela and Plachetka, Annette and Shanower, Gregory and Tolstorukov, Michael Y. and Luquette, Lovelace J. and Xi, Ruibin and Jung, Youngsook L. and Park, Richard W. and Bishop, Eric P. and Canfield, Theresa P. and Sandstrom, Richard and Thurman, Robert E. and MacAlpine, David M. and Stamatoyannopoulos, John A. and Kellis, Manolis and Elgin, Sarah C. R. and Kuroda, Mitzi I. and Pirrotta, Vincenzo and Karpen, Gary H. and Park, Peter J.},
