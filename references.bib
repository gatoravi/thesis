@article{vlachj_amatib:PhosphorylationdependentDegradation1997,
  title = {Phosphorylation-Dependent Degradation of the Cyclin-Dependent Kinase Inhibitor {{p27Kip1}}},
  author = {Vlach, Jaromir and Hennecke, Silke and Amati, Bruno},
