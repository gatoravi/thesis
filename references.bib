	author = {Chuffart, Florent and Richard, Magali and Jost, Daniel and Burny, Claire and Duplus-Bottin, Hélène and Ohya, Yoshikazu and Yvert, Gaël},
	month = aug,
	year = {2016},
	keywords = {Genetic loci, Phenotypes, Genetic polymorphism, Quantitative trait loci, Galactose, Gene mapping, Heredity, Yeast},
	pages = {e1006213},
	file = {Full Text PDF:/Users/aramu/Zotero/storage/VCVDGIY4/Chuffart et al. - 2016 - Exploiting Single-Cell Quantitative Data to Map Ge.pdf:application/pdf;Snapshot:/Users/aramu/Zotero/storage/LN2MV22Z/article.html:text/html}
}

@article{hanna_direct_2009,
