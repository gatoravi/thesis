}

@article{snellk_weberg:EnzymicImbalance1988,
  title = {Enzymic Imbalance in Serine Metabolism in Human Colon Carcinoma and Rat Sarcoma.},
  author = {Snell, K. and Natsumeda, Y. and Eble, J. N. and Glover, J. L. and Weber, G.},
