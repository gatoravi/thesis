  author = {Prasanth, Kannanganattu V. and Prasanth, Supriya G. and Xuan, Zhenyu and Hearn, Stephen and Freier, Susan M. and Bennett, C. Frank and Zhang, Michael Q. and Spector, David L.},
  year = {2005},
