@article{lupianezdg_mundloss:DisruptionsTopological2015,
  title = {Disruptions of Topological Chromatin Domains Cause Pathogenic Rewiring of Gene-Enhancer Interactions},
  author = {Lupi{\'a}{\~n}ez, Dar{\'i}o G. and Kraft, Katerina and Heinrich, Verena and Krawitz, Peter and Brancati, Francesco and Klopocki, Eva and Horn, Denise and Kayserili, H{\"u}lya and Opitz, John M. and Laxova, Renata and {Santos-Simarro}, Fernando and {Gilbert-Dussardier}, Brigitte and Wittler, Lars and Borschiwer, Marina and Haas, Stefan A. and Osterwalder, Marco and Franke, Martin and Timmermann, Bernd and Hecht, Jochen and Spielmann, Malte and Visel, Axel and Mundlos, Stefan},
