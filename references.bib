@article{sanchezy_huartem:GenomewideAnalysis2014,
  title = {Genome-Wide Analysis of the Human P53 Transcriptional Network Unveils a {{lncRNA}} Tumour Suppressor Signature},
  author = {S{\'a}nchez, Yolanda and Segura, Victor and {Mar{\'i}n-B{\'e}jar}, Oskar and Athie, Alejandro and Marchese, Francesco P. and Gonz{\'a}lez, Jovanna and Bujanda, Luis and Guo, Shuling and Matheu, Ander and Huarte, Maite},
  year = {2014},
  month = dec,
  journal = {Nature Communications},
  volume = {5},
  doi = {10.1038/ncomms6812},
  abstract = {Despite the inarguable relevance of \hspace{0pt}p53 in cancer, genome-wide studies relating endogenous \hspace{0pt}p53 activity to the expression of lncRNAs in human cells are still missing. Here, by integrating RNA-seq with \hspace{0pt}p53 ChIP-seq analyses of a human cancer cell line under DNA damage, we define a high-confidence set of 18 lncRNAs that are \hspace{0pt}p53 transcriptional targets. We demonstrate that two of the \hspace{0pt}p53-regulated lncRNAs are required for the efficient binding of \hspace{0pt}p53 to some of its target genes, modulating the \hspace{0pt}p53 transcriptional network and contributing to apoptosis induction by DNA damage. We also show that the expression of \hspace{0pt}p53-lncRNAs is lowered in colorectal cancer samples, constituting a tumour suppressor signature with high diagnostic power. Thus, \hspace{0pt}p53-regulated lncRNAs establish a positive regulatory feedback loop that enhances \hspace{0pt}p53 tumour suppressor activity. Furthermore, the signature defined by \hspace{0pt}p53-regulated lncRNAs supports their potential use in the clinic as biomarkers and therapeutic targets. View full text},
  copyright = {\textcopyright{} 2014 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
  langid = {english},
  keywords = {Biological sciences,Cancer,Molecular biology},
