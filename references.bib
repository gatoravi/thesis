@article{wendtks_petersjm:CohesinMediates2008,
  title = {Cohesin Mediates Transcriptional Insulation by {{CCCTC-binding}} Factor},
  author = {Wendt, Kerstin S. and Yoshida, Keisuke and Itoh, Takehiko and Bando, Masashige and Koch, Birgit and Schirghuber, Erika and Tsutsumi, Shuichi and Nagae, Genta and Ishihara, Ko and Mishiro, Tsuyoshi and Yahata, Kazuhide and Imamoto, Fumio and Aburatani, Hiroyuki and Nakao, Mitsuyoshi and Imamoto, Naoko and Maeshima, Kazuhiro and Shirahige, Katsuhiko and Peters, Jan-Michael},
