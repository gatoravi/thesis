@article{moudgila_mitrard:SelfReportingTransposons2020,
  title = {Self-{{Reporting Transposons Enable Simultaneous~Readout}} of {{Gene Expression}} and {{Transcription~Factor Binding}} in {{Single Cells}}},
  author = {Moudgil, Arnav and Wilkinson, Michael N. and Chen, Xuhua and He, June and Cammack, Alexander J. and Vasek, Michael J. and Lagunas, Tom{\'a}s and Qi, Zongtai and Lalli, Matthew A. and Guo, Chuner and Morris, Samantha A. and Dougherty, Joseph D. and Mitra, Robi D.},
