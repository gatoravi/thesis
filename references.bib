@article{ananiac_lupianezdg:VivoDissection2021,
  title = {In Vivo Dissection of a Clustered-{{CTCF}} Domain Boundary Reveals Developmental Principles of Regulatory Insulation},
  author = {Anania, Chiara and Acemel, Rafael D. and Jedamzick, Johanna and Bolondi, Adriano and Cova, Giulia and Brieske, Norbert and K{\"u}hn, Ralf and Wittler, Lars and Real, Francisca M. and Lupi{\'a}{\~n}ez, Dar{\'i}o G.},
