}

@article{talbotd_greavesdr:DominantControl1989,
  title = {A Dominant Control Region from the Human {$\beta$}-Globin Locus Conferring Integration Site-Independent Gene Expression},
  author = {Talbot, Dale and Collis, Philip and Antoniou, Michael and Vidal, Miguel and Grosveld, Frank and Greaves, David R.},
