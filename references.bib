	file = {Full Text PDF:/Users/aramu/Zotero/storage/2DENFLGQ/Araya et al. - 2012 - A fundamental protein property, thermodynamic stab.pdf:application/pdf;Snapshot:/Users/aramu/Zotero/storage/9FE9DY9M/16858.html:text/html}
