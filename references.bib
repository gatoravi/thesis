  keywords = {Cloning,Fluorescent in situ hybridization,Gene expression,HIV,HIV-1,Human genomics,Messenger RNA,Protein expression},
  file = {C\:\\Users\\Rilakkuma\\Zotero\\storage\\ZMQPP9LU\\Dar et al. - 2016 - Transcriptional Bursting Explains the Noise–Versus.pdf;C\:\\Users\\Rilakkuma\\Zotero\\storage\\RL2RU7HJ\\article.html}
}

