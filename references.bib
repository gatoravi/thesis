@article{darrd_weinbergerls:TranscriptionalBursting2016,
  title = {Transcriptional {{Bursting Explains}} the {{Noise}}\textendash{{Versus}}\textendash{{Mean Relationship}} in {{mRNA}} and {{Protein Levels}}},
  author = {Dar, Roy D. and Shaffer, Sydney M. and Singh, Abhyudai and Razooky, Brandon S. and Simpson, Michael L. and Raj, Arjun and Weinberger, Leor S.},
