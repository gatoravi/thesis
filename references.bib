@article{huangh_renb:CTCFMediates2021,
  title = {{{CTCF}} Mediates Dosage- and Sequence-Context-Dependent Transcriptional Insulation by Forming Local Chromatin Domains},
  author = {Huang, Hui and Zhu, Quan and Jussila, Adam and Han, Yuanyuan and Bintu, Bogdan and Kern, Colin and Conte, Mattia and Zhang, Yanxiao and Bianco, Simona and Chiariello, Andrea M. and Yu, Miao and Hu, Rong and Tastemel, Melodi and Juric, Ivan and Hu, Ming and Nicodemi, Mario and Zhuang, Xiaowei and Ren, Bing},
