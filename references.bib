@article{mullan_navaracs:DifferentialLocalization2014,
  title = {Differential Localization and High Expression of {{SURVIVIN}} Splice Variants in Human Embryonic Stem Cells but Not in Differentiated Cells Implicate a Role for {{SURVIVIN}} in Pluripotency},
