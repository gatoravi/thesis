  pages = {477--486},
  publisher = {{Nature Publishing Group}},
  issn = {1546-1718},
  doi = {10.1038/s41588-021-00816-z},
  abstract = {Acquisition of cell fate is thought to rely on the specific interaction of remote cis-regulatory modules (CRMs), for example, enhancers and target promoters. However, the precise interplay between chromatin structure and gene expression is still unclear, particularly within multicellular developing organisms. In the present study, we employ Hi-M, a single-cell spatial genomics approach, to detect CRM\textendash promoter looping interactions within topologically associating domains (TADs) during early Drosophila development. By comparing cis-regulatory loops in alternate cell types, we show that physical proximity does not necessarily instruct transcriptional states. Moreover, multi-way analyses reveal that multiple CRMs spatially coalesce to form hubs. Loops and CRM hubs are established early during development, before the emergence of TADs. Moreover, CRM hubs are formed, in part, via the action of the pioneer transcription factor Zelda and precede transcriptional activation. Our approach provides insight into the role of CRM\textendash promoter interactions in defining transcriptional states, as well as distinct cell types.},
  copyright = {2021 The Author(s), under exclusive licence to Springer Nature America, Inc.},
  langid = {english},
  file = {C\:\\Users\\Rilakkuma\\Zotero\\storage\\U9MRI7R4\\Espinola et al. - 2021 - Cis -regulatory chromatin loops arise before TADs .pdf;C\:\\Users\\Rilakkuma\\Zotero\\storage\\JV4KSLYU\\s41588-021-00816-z.html}
}

@article{essafia_hohensteinp:Wt1ControlledChromatin2011,
  title = {A {{Wt1-Controlled Chromatin Switching Mechanism Underpins Tissue-Specific Wnt4 Activation}} and {{Repression}}},
