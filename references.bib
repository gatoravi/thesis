@article{zhouq_tenendg:ZNF143Mediates2021,
  title = {{{ZNF143}} Mediates {{CTCF-bound}} Promoter\textendash Enhancer Loops Required for Murine Hematopoietic Stem and Progenitor Cell Function},
  author = {Zhou, Qiling and Yu, Miao and {Tirado-Magallanes}, Roberto and Li, Bin and Kong, Lingshi and Guo, Mingrui and Tan, Zi Hui and Lee, Sanghoon and Chai, Li and Numata, Akihiko and Benoukraf, Touati and Fullwood, Melissa Jane and Osato, Motomi and Ren, Bing and Tenen, Daniel G.},
  year = {2021},
  month = jan,
  journal = {Nature Communications},
  volume = {12},
  number = {1},
  pages = {43},
  publisher = {{Nature Publishing Group}},
  issn = {2041-1723},
  doi = {10.1038/s41467-020-20282-1},
  abstract = {CCCTC binding factor (CTCF) is an important factor in the maintenance of chromatin\textendash chromatin interactions, yet the mechanism regulating its binding to chromatin is unknown. We demonstrate that zinc finger protein 143 (ZNF143) is a key regulator for CTCF-bound promoter\textendash enhancer loops. In the murine genome, a large percentage of CTCF and ZNF143 DNA binding motifs are distributed 37\,bp apart in the convergent orientation. Furthermore, deletion of ZNF143 leads to loss of CTCF binding on promoter and enhancer regions associated with gene expression changes. CTCF-bound promoter\textendash enhancer loops are also disrupted after excision of ZNF143. ZNF143-CTCF-bound promoter\textendash enhancer loops regulate gene expression patterns essential for maintenance of murine hematopoietic stem and progenitor cell integrity. Our data suggest a common feature of gene regulation is~that ZNF143 is a critical factor for CTCF-bound promoter\textendash enhancer loops.},
  copyright = {2021 The Author(s)},
  langid = {english},
  file = {C\:\\Users\\Rilakkuma\\Zotero\\storage\\KN4H9DZN\\Zhou et al. - 2021 - ZNF143 mediates CTCF-bound promoter–enhancer loops.pdf;C\:\\Users\\Rilakkuma\\Zotero\\storage\\3IVKLSEJ\\s41467-020-20282-1.html}
}

