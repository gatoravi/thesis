  shorttitle = {Production of Transgenic Maize from Bombarded Type {{II}} Callus},
  author = {Frame, Bronwyn R. and Zhang, Hongyi and Cocciolone, Suzy M. and Sidorenko, Lyudmila V. and Dietrich, Charles R. and Pegg, Sue Ellen and Zhen, Shifu and Schnable, Patrick S. and Wang, Kan},
