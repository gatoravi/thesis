  pmid = {16086016},
  keywords = {Chromosomes; Fungal,DNA Replication,Gene Dosage,Gene Expression Regulation; Fungal,Kinetics,Models; Genetic,Plasmids,Promoter Regions; Genetic,Saccharomyces cerevisiae,Saccharomyces cerevisiae Proteins,Statistical Distributions,Stochastic Processes,Transcription; Genetic,Transcriptional Activation}
}

@article{bedbrookcn_gradinaruv:ViralStrategies2018,
