	number = {5},
	urldate = {2018-05-21},
	journal = {Molecular Systems Biology},
	author = {Ravarani, Charles NJ and Erkina, Tamara Y. and Baets, Greet De and Dudman, Daniel C. and Erkine, Alexandre M. and Babu, M. Madan},
	month = may,
	year = {2018},
	pmid = {29759983},
	keywords = {machine learning, high‐throughput screen, intrinsically disordered protein, mutational scanning, transactivation domain},
	pages = {e8190},
	file = {Full Text PDF:/Users/aramu/Zotero/storage/TJBNTRAC/Ravarani et al. - 2018 - High‐throughput discovery of functional disordered.pdf:application/pdf;Snapshot:/Users/aramu/Zotero/storage/QZ5JBNEH/e8190.html:text/html}
