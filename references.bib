@article{gyurkovicsh_karchf:NewHomeotic1990,
  title = {A New Homeotic Mutation in the {{Drosophila}} Bithorax Complex Removes a Boundary Separating Two Domains of Regulation},
  author = {Gyurkovics, H. and Gausz, J. and Kummer, J. and Karch, F.},
  year = {1990},
