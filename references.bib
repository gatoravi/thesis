@article{wangc_zhuangx:ImagingbasedPooled2019,
  title = {Imaging-Based Pooled {{CRISPR}} Screening Reveals Regulators of {{lncRNA}} Localization},
  author = {Wang, Chong and Lu, Tian and Emanuel, George and Babcock, Hazen P. and Zhuang, Xiaowei},
