@article{nguyenlv_eavescj:CancerStem2012,
  title = {Cancer Stem Cells: An Evolving Concept},
  shorttitle = {Cancer Stem Cells},
  author = {Nguyen, Long V. and Vanner, Robert and Dirks, Peter and Eaves, Connie J.},
  year = {2012},
  month = feb,
  journal = {Nature Reviews Cancer},
  volume = {12},
  number = {2},
  pages = {133--143},
  issn = {1474-175X},
  doi = {10.1038/nrc3184},
  abstract = {The cancer stem cell (CSC) concept derives from the fact that cancers are dysregulated tissue clones whose continued propagation is vested in a biologically distinct subset of cells that are typically rare. This idea is not new, but has recently gained prominence because of advances in defining normal tissue hierarchies, a greater appreciation of the multistep nature of oncogenesis and improved methods to propagate primary human cancers in immunodeficient mice. As a result we have obtained new insights into why the CSC concept is not universally applicable, as well as a new basis for understanding the complex evolution, phenotypic heterogeneity and therapeutic challenges of many human cancers.},
