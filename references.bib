  author = {Liao, Xing-Hua and Wang, Nan and Liu, Long-Yue and Zheng, Li and Xing, Wen-Jing and Zhao, Dong-Wei and Sun, Xue-Guang and Hu, Peng and Dong, Jian and Zhang, Tong-Cun},
