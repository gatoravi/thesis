@article{sankara_helink:HistoneEditing2022,
  title = {Histone Editing Elucidates the Functional Roles of {{H3K27}} Methylation and Acetylation in Mammals},
  author = {Sankar, Aditya and Mohammad, Faizaan and Sundaramurthy, Arun Kumar and Wang, Hua and Lerdrup, Mads and Tatar, Tulin and Helin, Kristian},
