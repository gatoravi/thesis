@article{zhaoz_duz:SinglecellDynamics2021,
  title = {Single-Cell Dynamics of Chromatin Activity during Cell Lineage Differentiation in {{Caenorhabditis}} Elegans Embryos},
  author = {Zhao, Zhiguang and Fan, Rong and Xu, Weina and Kou, Yahui and Wang, Yangyang and Ma, Xuehua and Du, Zhuo},
