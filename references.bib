@article{wroblewskit_michelmorer:OptimizationAgrobacteriummediated2005,
  title = {Optimization of {{Agrobacterium-mediated}} Transient Assays of Gene Expression in Lettuce, Tomato and {{Arabidopsis}}},
  author = {Wroblewski, Tadeusz and Tomczak, Anna and Michelmore, Richard},
  year = {2005},
