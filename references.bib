  langid = {english},
  pmid = {33268361},
  file = {C\:\\Users\\Rilakkuma\\Zotero\\storage\\BXL9QYBI\\Poul et al. - 2020 - Regulatory encoding of quantitative variation in s.pdf;C\:\\Users\\Rilakkuma\\Zotero\\storage\\87EQJGN4\\eabe2955.html}
