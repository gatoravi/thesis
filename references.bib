@article{jongj_ridderj:ChromatinLandscapes2014,
  title = {Chromatin {{Landscapes}} of {{Retroviral}} and {{Transposon Integration Profiles}}},
  author = {de Jong, Johann and Akhtar, Waseem and Badhai, Jitendra and Rust, Alistair G. and Rad, Roland and Hilkens, John and Berns, Anton and van Lohuizen, Maarten and Wessels, Lodewyk F. A. and de Ridder, Jeroen},
  year = {2014},
  month = apr,
  journal = {PLOS Genetics},
  volume = {10},
  number = {4},
  pages = {e1004250},
  publisher = {{Public Library of Science}},
  issn = {1553-7404},
  doi = {10.1371/journal.pgen.1004250},
  abstract = {The ability of retroviruses and transposons to insert their genetic material into host DNA makes them widely used tools in molecular biology, cancer research and gene therapy. However, these systems have biases that may strongly affect research outcomes. To address this issue, we generated very large datasets consisting of to unselected integrations in the mouse genome for the Sleeping Beauty (SB) and piggyBac (PB) transposons, and the Mouse Mammary Tumor Virus (MMTV). We analyzed (epi)genomic features to generate bias maps at both local and genome-wide scales. MMTV showed a remarkably uniform distribution of integrations across the genome. More distinct preferences were observed for the two transposons, with PB showing remarkable resemblance to bias profiles of the Murine Leukemia Virus. Furthermore, we present a model where target site selection is directed at multiple scales. At a large scale, target site selection is similar across systems, and defined by domain-oriented features, namely expression of proximal genes, proximity to CpG islands and to genic features, chromatin compaction and replication timing. Notable differences between the systems are mainly observed at smaller scales, and are directed by a diverse range of features. To study the effect of these biases on integration sites occupied under selective pressure, we turned to insertional mutagenesis (IM) screens. In IM screens, putative cancer genes are identified by finding frequently targeted genomic regions, or Common Integration Sites (CISs). Within three recently completed IM screens, we identified 7\%\textendash 33\% putative false positive CISs, which are likely not the result of the oncogenic selection process. Moreover, results indicate that PB, compared to SB, is more suited to tag oncogenes.},
  langid = {english},
  keywords = {Cancers and neoplasms,Chromatin,DNA methylation,Gene expression,Mammalian genomics,Sequence alignment,Site selection,Transposable elements},
  file = {C\:\\Users\\Rilakkuma\\Zotero\\storage\\29TPAGZP\\Jong et al. - 2014 - Chromatin Landscapes of Retroviral and Transposon .pdf;C\:\\Users\\Rilakkuma\\Zotero\\storage\\BLQI4945\\article.html}
}

@article{joshis_chircopm:DynaminInhibitors2011,
  title = {Dynamin Inhibitors Induce Caspase-Mediated Apoptosis Following Cytokinesis Failure in Human Cancer Cells and This Is Blocked by {{Bcl-2}} Overexpression},
  author = {Joshi, Sanket and Braithwaite, Antony W. and Robinson, Phillip J. and Chircop, Megan},
  year = {2011},
  month = jun,
  journal = {Molecular Cancer},
  volume = {10},
  number = {1},
  pages = {78},
  issn = {1476-4598},
  doi = {10.1186/1476-4598-10-78},
  abstract = {The aim of both classical (e.g. taxol) and targeted anti-mitotic agents (e.g. Aurora kinase inhibitors) is to disrupt the mitotic spindle. Such compounds are currently used in the clinic and/or are being tested in clinical trials for cancer treatment. We recently reported a new class of targeted anti-mitotic compounds that do not disrupt the mitotic spindle, but exclusively block completion of cytokinesis. This new class includes MiTMAB and OcTMAB (MiTMABs), which are potent inhibitors of the endocytic protein, dynamin. Like other anti-mitotics, MiTMABs are highly cytotoxic and possess anti-proliferative properties, which appear to be selective for cancer cells. The cellular response following cytokinesis failure and the mechanistic pathway involved is unknown.},
  copyright = {2011 Joshi et al; licensee BioMed Central Ltd.},
  langid = {english},
  pmid = {21708043},
  file = {C\:\\Users\\Rilakkuma\\Zotero\\storage\\FIQ8G2XJ\\Joshi et al. - 2011 - Dynamin inhibitors induce caspase-mediated apoptos.pdf;C\:\\Users\\Rilakkuma\\Zotero\\storage\\JW3G8MCZ\\78.html}
}

