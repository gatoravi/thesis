@article{hul_guanxy:AssociationVimentin2004,
  title = {Association of {{Vimentin}} Overexpression and Hepatocellular Carcinoma Metastasis},
  author = {Hu, Liang and Lau, Sze Hang and Tzang, Chi-Hung and Wen, Jian-Ming and Wang, Weisheng and Xie, Dan and Huang, Minghui and Wang, Yi and Wu, Meng-Chao and Huang, Jie-Fu and Zeng, Wei-Fen and Sham, Jonathan S. T. and Yang, Mengsu and Guan, Xin-Yuan},
