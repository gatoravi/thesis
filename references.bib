@article{limcy_kadonagajt:MTENew2004,
  title = {The {{MTE}}, a New Core Promoter Element for Transcription by {{RNA}} Polymerase {{II}}},
  author = {Lim, Chin Yan and Santoso, Buyung and Boulay, Thomas and Dong, Emily and Ohler, Uwe and Kadonaga, James T.},
