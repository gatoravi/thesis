@article{bandoh_koiket:Phosphorylation6phosphofructo2Kinase2005,
  title = {Phosphorylation of the 6-{{Phosphofructo-2-Kinase}}/{{Fructose}} 2,6-{{Bisphosphatase}}/{{PFKFB3 Family}} of {{Glycolytic Regulators}} in {{Human Cancer}}},
  author = {Bando, Hidenori and Atsumi, Toshiya and Nishio, Taro and Niwa, Hirokatsu and Mishima, Shinya and Shimizu, Chikara and Yoshioka, Narihito and Bucala, Richard and Koike, Takao},
  year = {2005},
  month = aug,
  journal = {Clinical Cancer Research},
