@article{bonnyar_el-samadh:OrthogonalControl2021,
  title = {Orthogonal Control of Mean and Variability of Endogenous Genes in a Human Cell Line},
  author = {Bonny, Alain R. and Fonseca, Jo{\~a}o Pedro and Park, Jesslyn E. and {El-Samad}, Hana},
  year = {2021},
