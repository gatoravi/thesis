	pages = {470--483.e8},
	file = {ScienceDirect Full Text PDF:/Users/aramu/Zotero/storage/V2Z5G3XR/Shen et al. - 2018 - Divergence in DNA Specificity among Paralogous Tra.pdf:application/pdf;ScienceDirect Snapshot:/Users/aramu/Zotero/storage/8F5NXHT7/S2405471218300620.html:text/html}
