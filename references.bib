  publisher = {{Nature Publishing Group}},
  issn = {1471-0080},
  doi = {10.1038/nrm3949},
  abstract = {Enhancers are genetic elements that have major roles in determining cell type-specific gene expression patterns and responses to internal and external signals.Mammalian genomes contain millions of enhancers, but only a subset of them are selected and activated in each cell type in the body.Enhancer selection involves collaborative interactions between 'pioneer' or lineage-determining transcription factors. Such factors are able to prime enhancers in a cell type-specific manner by binding to closely spaced recognition motifs.Signal-dependent transcription factors, such as nuclear receptors and nuclear factor-{$\kappa$}B (NF-{$\kappa$}B), primarily bind to regions of the genome that are primed in a cell type-specific manner by lineage-determining transcription factors. This enables such broadly expressed, signal-dependent transcription factors to regulate gene expression in a cell type-specific manner.A proportion of the genome contains a very high density of marks of active enhancers. Such enhancer-rich genomic regions, which are called super-enhancers, are different in each cell type and regulate genes required for establishing cell identity and function.An understanding of the mechanisms underlying the cell type-specific selection and function of enhancers will improve our understanding of the effects of natural genetic variation on complex phenotypes and diseases.},
  copyright = {2015 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
  langid = {english},
  file = {C\:\\Users\\Rilakkuma\\Zotero\\storage\\GX3HBNGD\\Heinz et al. - 2015 - The selection and function of cell type-specific e.pdf;C\:\\Users\\Rilakkuma\\Zotero\\storage\\F8SS2WAX\\nrm3949.html}
}

@article{heleniuse_teerith:GeneDelivery2000,
  title = {Gene Delivery into Intact Plants Using the {{HeliosTM}} Gene Gun},
  author = {Helenius, Elina and Boije, Maria and {Niklander-Teeri}, Viola and Palva, E. Tapio and Teeri, Teemu H.},
  year = {2000},
  month = sep,
  journal = {Plant Molecular Biology Reporter},
  volume = {18},
  number = {3},
  pages = {287--288},
  issn = {0735-9640, 1572-9818},
  doi = {10.1007/BF02824002},
  abstract = {Particle bombardment is a physical method for cell transformation in which high density, sub-cellular sized particles are accelerated to high velocity to carry DNA/RNA into living cells. It is a versatile technique that can be used for transient expression studies as well as for creating stable transformants. The HeliosTM gene gun uses DNA-coated gold particles that are precipitated on the inner wall of a plastic tube and accelerated by pressurized helium. It differs from previous particle bombardment methods since it operates in ambient pressure and can be used. e.g. in the field. The aim of this study was to optimize parameters for transient expression of gene constructs into arabidopsis, tobacco and birch with the HeliosTM Gene Gun. In order to investigate transient gene delivery, we used constructs containing the constitutively active promoter of the CaMV 35S transcript fused with reporter genes encoding luciferase (LUC) and {$\beta$}-glucuronidase (GUS). Optimization was performed in a step-by-step manner. We found that the most critical parameters were helium pressure, the optimum of which varied between plant species, and the amount of gold. Gold particles with a diameter of 0.6 {$\mu$}m were best for all plant species studied. The optimization procedure helped to increase the expression levels five- to ten-fold.},
  langid = {english},
  keywords = {bioinformatics,metabolomics,optimization,particle bombardment,plant biotechnology,Plant Breeding/Biotechnology,Plant Sciences,proteomics,transient expression},
  file = {C\:\\Users\\Rilakkuma\\Zotero\\storage\\35447JWW\\BF02824002.html}
}

@article{henleymj_mappak:UnexpectedSpecificity2020,
  title = {Unexpected Specificity within Dynamic Transcriptional Protein\textendash Protein Complexes},
  author = {Henley, Matthew J. and Linhares, Brian M. and Morgan, Brittany S. and Cierpicki, Tomasz and Fierke, Carol A. and Mapp, Anna K.},
  year = {2020},
  month = nov,
  journal = {Proceedings of the National Academy of Sciences},
  volume = {117},
  number = {44},
  pages = {27346--27353},
  publisher = {{National Academy of Sciences}},
  issn = {0027-8424, 1091-6490},
  doi = {10.1073/pnas.2013244117},
  abstract = {A key functional event in eukaryotic gene activation is the formation of dynamic protein\textendash protein interaction networks between transcriptional activators and transcriptional coactivators. Seemingly incongruent with the tight regulation of transcription, many biochemical and biophysical studies suggest that activators use nonspecific hydrophobic and/or electrostatic interactions to bind to coactivators, with few if any specific contacts. Here a mechanistic dissection of a set of representative dynamic activator\textbullet coactivator complexes, comprised of the ETV/PEA3 family of activators and the coactivator Med25, reveals a different molecular recognition model. The data demonstrate that small sequence variations within an activator family significantly redistribute the conformational ensemble of the complex while not affecting overall affinity, and distal residues within the activator\textemdash not often considered as contributing to binding\textemdash play a key role in mediating conformational redistribution. The ETV/PEA3\textbullet Med25 ensembles are directed by specific contacts between the disordered activator and the Med25 interface, which is facilitated by structural shifts of the coactivator binding surface. Taken together, these data highlight the critical role coactivator plasticity plays in recognition of disordered activators and indicate that molecular recognition models of disordered proteins must consider the ability of the binding partners to mediate specificity.},
  chapter = {Biological Sciences},
  copyright = {Copyright \textcopyright{} 2020 the Author(s). Published by PNAS.. https://creativecommons.org/licenses/by-nc-nd/4.0/This open access article is distributed under Creative Commons Attribution-NonCommercial-NoDerivatives License 4.0 (CC BY-NC-ND).},
  langid = {english},
  pmid = {33077600},
  keywords = {coactivator,ETV/PEA3,Med25,protein–protein interactions,transcriptional activator},
  file = {C\:\\Users\\Rilakkuma\\Zotero\\storage\\CZAY63ZC\\Henley et al. - 2020 - Unexpected specificity within dynamic transcriptio.pdf;C\:\\Users\\Rilakkuma\\Zotero\\storage\\NCIZEY37\\27346.html}
}

