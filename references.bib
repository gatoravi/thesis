@article{gaoy_soderlingsh:PlugandPlayProtein2019,
  title = {Plug-and-{{Play Protein Modification Using Homology-Independent Universal Genome Engineering}}},
  author = {Gao, Yudong and Hisey, Erin and Bradshaw, Tyler W.A. and Erata, Eda and Brown, Walter E. and Courtland, Jamie L. and Uezu, Akiyoshi and Xiang, Yu and Diao, Yarui and Soderling, Scott H.},
