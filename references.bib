  file = {C\:\\Users\\Rilakkuma\\Zotero\\storage\\ZUR2SEBM\\Choy et al. - 2004 - MorA Defines a New Class of Regulators Affecting F.pdf;C\:\\Users\\Rilakkuma\\Zotero\\storage\\Q2PAJ5GX\\Choy et al. - 2004 - MorA Defines a New Class of Regulators Affecting F.html}
