  author = {Waldron, T. and De Dominici, M. and Soliera, A. R. and Audia, A. and Iacobucci, I. and Lonetti, A. and Martinelli, G. and Zhang, Y. and Martinez, R. and Hyslop, T. and Bender, T. P. and Calabretta, B.},
  year = {2012},
