  pmid = {9013546},
  file = {C\:\\Users\\Rilakkuma\\Zotero\\storage\\TVBSHP75\\Chu et al. - 1997 - MyoD Functions as a Transcriptional Repressor in P.pdf;C\:\\Users\\Rilakkuma\\Zotero\\storage\\7RSXR996\\3145.html}
}

@article{chungh_regeva:SimultaneousSingle2021,
