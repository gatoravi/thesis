  keywords = {Animals,Chromatin,Computer Graphics,Databases; Genetic,Genomics,Humans,K562 Cells,Mice,Molecular Sequence Annotation,Software,Thymocytes},
  file = {C\:\\Users\\Rilakkuma\\Zotero\\storage\\59TQHPSU\\Harmston et al. - 2015 - GenomicInteractions An RBioconductor package for.pdf}
