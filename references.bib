  langid = {english},
  keywords = {Hepatocellular carcinoma,Metastasis,microarray,Vimentin},
  file = {C\:\\Users\\Rilakkuma\\Zotero\\storage\\H3G5I6BK\\Hu et al. - 2004 - Association of Vimentin overexpression and hepatoc.pdf;C\:\\Users\\Rilakkuma\\Zotero\\storage\\8MFAKX29\\1206483a.html}
