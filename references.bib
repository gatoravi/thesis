}

@article{dinzeos_coppaa:NovelHuman2012,
  title = {A Novel Human {{Smad4}} Mutation Is Involved in Papillary Thyroid Carcinoma Progression},
  author = {D'Inzeo, Sonia and Nicolussi, Arianna and Donini, Caterina Francesca and Zani, Massimo and Mancini, Patrizia and Nardi, Francesco and Coppa, Anna},
  year = {2012},
