@article{karg_teichmannsa:FlippingPolycomb2017,
  title = {Flipping between {{Polycomb}} Repressed and Active Transcriptional States Introduces Noise in Gene Expression},
  author = {Kar, Gozde and Kim, Jong Kyoung and Kolodziejczyk, Aleksandra A. and Natarajan, Kedar Nath and Torlai Triglia, Elena and Mifsud, Borbala and Elderkin, Sarah and Marioni, John C. and Pombo, Ana and Teichmann, Sarah A.},
  year = {2017},
