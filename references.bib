}

@article{ozbudak_regulation_2002,
	title = {Regulation of noise in the expression of a single gene},
	volume = {31},
	copyright = {2002 Nature Publishing Group},
	issn = {1546-1718},
	url = {https://www.nature.com/articles/ng869z},
	doi = {10.1038/ng869},
	abstract = {Stochastic mechanisms are ubiquitous in biological systems. Biochemical reactions that involve small numbers of molecules are intrinsically noisy, being dominated by large concentration fluctuations1,2,3. This intrinsic noise has been implicated in the random lysis/lysogeny decision of bacteriophage-λ4, in the loss of synchrony of circadian clocks5,6 and in the decrease of precision of cell signals7. We sought to quantitatively investigate the extent to which the occurrence of molecular fluctuations within single cells (biochemical noise) could explain the variation of gene expression levels between cells in a genetically identical population (phenotypic noise). We have isolated the biochemical contribution to phenotypic noise from that of other noise sources by carrying out a series of differential measurements. We varied independently the rates of transcription and translation of a single fluorescent reporter gene in the chromosome of Bacillus subtilis, and we quantitatively measured the resulting changes in the phenotypic noise characteristics. We report that of these two parameters, increased translational efficiency is the predominant source of increased phenotypic noise. This effect is consistent with a stochastic model of gene expression in which proteins are produced in random and sharp bursts. Our results thus provide the first direct experimental evidence of the biochemical origin of phenotypic noise, demonstrating that the level of phenotypic variation in an isogenic population can be regulated by genetic parameters.},
	language = {en},
	number = {1},
	urldate = {2019-03-13},
	journal = {Nature Genetics},
	author = {Ozbudak, Ertugrul M. and Thattai, Mukund and Kurtser, Iren and Grossman, Alan D. and van Oudenaarden, Alexander},
	month = may,
	year = {2002},
	pages = {69--73},
	file = {Full Text PDF:/Users/aramu/Zotero/storage/SLFG5PIR/Ozbudak et al. - 2002 - Regulation of noise in the expression of a single .pdf:application/pdf;Snapshot:/Users/aramu/Zotero/storage/PLAKBU3W/ng869z.html:text/html}
}

@article{swain_intrinsic_2002,
	title = {Intrinsic and extrinsic contributions to stochasticity in gene expression},
	volume = {99},
	copyright = {Copyright © 2002, The National Academy of Sciences},
	issn = {0027-8424, 1091-6490},
	url = {https://www.pnas.org/content/99/20/12795},
	doi = {10.1073/pnas.162041399},
	abstract = {Gene expression is a stochastic, or “noisy,” process. This noise comes about in two ways. The inherent stochasticity of biochemical processes such as transcription and translation generates “intrinsic” noise. In addition, fluctuations in the amounts or states of other cellular components lead indirectly to variation in the expression of a particular gene and thus represent “extrinsic” noise. Here, we show how the total variation in the level of expression of a given gene can be decomposed into its intrinsic and extrinsic components. We demonstrate theoretically that simultaneous measurement of two identical genes per cell enables discrimination of these two types of noise. Analytic expressions for intrinsic noise are given for a model that involves all the major steps in transcription and translation. These expressions give the sensitivity to various parameters, quantify the deviation from Poisson statistics, and provide a way of fitting experiment. Transcription dominates the intrinsic noise when the average number of proteins made per mRNA transcript is greater than ≃2. Below this number, translational effects also become important. Gene replication and cell division, included in the model, cause protein numbers to tend to a limit cycle. We calculate a general form for the extrinsic noise and illustrate it with the particular case of a single fluctuating extrinsic variable—a repressor protein, which acts on the gene of interest. All results are confirmed by stochastic simulation using plausible parameters for Escherichia coli.},
	language = {en},
	number = {20},
	urldate = {2019-03-13},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Swain, Peter S. and Elowitz, Michael B. and Siggia, Eric D.},
	month = oct,
	year = {2002},
	pmid = {12237400},
	pages = {12795--12800},
	file = {Full Text PDF:/Users/aramu/Zotero/storage/8X8TLK6K/Swain et al. - 2002 - Intrinsic and extrinsic contributions to stochasti.pdf:application/pdf;Snapshot:/Users/aramu/Zotero/storage/B8CGDPPE/12795.html:text/html}
}

@article{volfson_origins_2006,
