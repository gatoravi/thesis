@article{benvenisted_sprould:TranscriptionFactor2014,
  title = {Transcription Factor Binding Predicts Histone Modifications in Human Cell Lines},
  author = {Benveniste, Dan and Sonntag, Hans-Joachim and Sanguinetti, Guido and Sproul, Duncan},
  year = {2014},
