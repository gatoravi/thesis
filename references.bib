}

@article{espinolasm_nollmannm:CisRegulatory2021,
  title = {Cis -Regulatory Chromatin Loops Arise before {{TADs}} and Gene Activation, and Are Independent of Cell Fate during Early {{Drosophila}} Development},
  author = {Espinola, Sergio Martin and G{\"o}tz, Markus and Bellec, Maelle and Messina, Olivier and Fiche, Jean-Bernard and Houbron, Christophe and Dejean, Matthieu and Reim, Ingolf and Cardozo Gizzi, Andr{\'e}s M. and Lagha, Mounia and Nollmann, Marcelo},
