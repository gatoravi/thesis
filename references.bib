	pmid = {22748001},
	pmcid = {PMC3476390},
	pages = {61},
	file = {PubMed Central Full Text PDF:/Users/aramu/Zotero/storage/CCXXRE2D/Zhao et al. - 2012 - Genome-wide association study identifies a materna.pdf:application/pdf}
}

