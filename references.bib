@article{chenw_deplanckeb:GenomewideMolecular2021,
  title = {Genome-Wide Molecular Recording Using {{Live-seq}}},
  author = {Chen, Wanze and {Guillaume-Gentil}, Orane and Dainese, Riccardo and Rainer, Pernille Yde and Zachara, Magda and G{\"a}belein, Christoph G. and Vorholt, Julia A. and Deplancke, Bart},
