@article{baileytl_baileytl:STREMEAccurate2021,
  title = {{{STREME}}: {{Accurate}} and Versatile Sequence Motif Discovery},
  shorttitle = {{{STREME}}},
  author = {Bailey, Timothy L.},
  year = {2021},
