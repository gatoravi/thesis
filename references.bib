@article{vandervlagj_otteap:TranscriptionalRepression2000,
  title = {Transcriptional {{Repression Mediated}} by {{Polycomb Group Proteins}} and {{Other Chromatin-associated Repressors Is Selectively Blocked}} by {{Insulators}}},
