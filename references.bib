  keywords = {Animals,Cell Nucleus,Epithelial Cells,Extracellular Space,Humans,Isometric Contraction,Mechanotransduction; Cellular,Mesoderm,Mice,Protein Transport,Stress; Mechanical,Trans-Activators,Transforming Growth Factor beta}
}

