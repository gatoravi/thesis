@article{fjosea_krausss:StructureEarly1992,
  title = {Structure and Early Embryonic Expression of the Zebrafish Engrailed-2 Gene},
  author = {Fjose, Anders and Nj{\o}lstad, P{\aa}l R. and Nornes, Svanhild and Molven, Anders and Krauss, Stefan},
  year = {1992},
