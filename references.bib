	pages = {e1000626},
	file = {Full Text PDF:/Users/aramu/Zotero/storage/V7HBPTF5/Mar and Quackenbush - 2009 - Decomposition of Gene Expression State Space Traje.pdf:application/pdf;Snapshot:/Users/aramu/Zotero/storage/NTQRDUZY/article.html:text/html}
