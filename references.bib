  pmid = {11959843},
  keywords = {Gene Silencing,miRNA,RNAi,shRNA,siRNA},
  file = {C\:\\Users\\Rilakkuma\\Zotero\\storage\\XK7ITXC8\\Paddison et al. - 2002 - Short hairpin RNAs (shRNAs) induce sequence-specif.pdf;C\:\\Users\\Rilakkuma\\Zotero\\storage\\ZGNRBMPZ\\948.html}
