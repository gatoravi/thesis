  author = {Li, Ming Guang and Katsura, Koji and Nomiyama, Hisayuki and Komaki, Ken-ichiro and {Ninomiya-Tsuji}, Jun and Matsumoto, Kunihiro and Kobayashi, Takayasu and Tamura, Shinri},
