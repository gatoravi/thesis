  langid = {english},
  file = {C\:\\Users\\Rilakkuma\\Zotero\\storage\\FSIYJINH\\Szabo et al. - 2019 - Principles of genome folding into topologically as.pdf;C\:\\Users\\Rilakkuma\\Zotero\\storage\\AKLVCRV4\\eaaw1668.html}
