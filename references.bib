@article{dimrigp_bandv:Bmi1Oncogene2002,
  title = {The {{Bmi-1 Oncogene Induces Telomerase Activity}} and {{Immortalizes Human Mammary Epithelial Cells}}},
  author = {Dimri, Goberdhan P. and Martinez, Jose-Luis and Jacobs, Jacqueline J. L. and Keblusek, Petra and Itahana, Koji and van Lohuizen, Maarten and Campisi, Judith and Wazer, David E. and Band, Vimla},
  year = {2002},
