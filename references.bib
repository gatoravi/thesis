	pmid = {21915116},
	keywords = {half‐life, operational strategy, pre‐mRNA, transcriptional response},
	pages = {529},
	file = {Full Text PDF:/Users/aramu/Zotero/storage/BPTHKB7J/Zeisel et al. - 2011 - Coupled pre‐mRNA and mRNA dynamics unveil operatio.pdf:application/pdf;Snapshot:/Users/aramu/Zotero/storage/MQYWZRF7/529.html:text/html}
}

