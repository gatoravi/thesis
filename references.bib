@article{garay-arroyoa_alvarez-buyllaer:HormoneSymphony2012,
  title = {Hormone Symphony during Root Growth and Development},
  author = {{Garay-Arroyo}, Adriana and De La Paz S{\'a}nchez, Mar{\'i}a and {Garc{\'i}a-Ponce}, Berenice and Azpeitia, Eugenio and {Alvarez-Buylla}, Elena R},
  year = {2012},
