@article{contec_vauryc:CouplingEnhancer2002,
  title = {Coupling of {{Enhancer}} and {{Insulator Properties Identified}} in {{Two Retrotransposons Modulates Their Mutagenic Impact}} on {{Nearby Genes}}},
  author = {Conte, Caroline and Dastugue, Bernard and Vaury, Chantal},
