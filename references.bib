	pages = {e2000640},
	file = {Full Text PDF:/Users/aramu/Zotero/storage/4KTHT4PN/Mojtahedi et al. - 2016 - Cell Fate Decision as High-Dimensional Critical St.pdf:application/pdf;Full Text PDF:/Users/aramu/Zotero/storage/Z386VYST/Mojtahedi et al. - 2016 - Cell Fate Decision as High-Dimensional Critical St.pdf:application/pdf;Snapshot:/Users/aramu/Zotero/storage/D5XAKWBG/article.html:text/html;Snapshot:/Users/aramu/Zotero/storage/UDSQ2TKP/article.html:text/html}
