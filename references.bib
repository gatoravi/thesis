}

@misc{krusek_vaquerizasjm:TransposableElements2019,
  title = {Transposable Elements Drive Reorganisation of {{3D}} Chromatin during Early Embryogenesis},
  author = {Kruse, Kai and D{\'i}az, Noelia and {Enriquez-Gasca}, Rocio and Gaume, Xavier and {Torres-Padilla}, Maria-Elena and Vaquerizas, Juan M.},
  year = {2019},
  month = jan,
  pages = {523712},
  publisher = {{bioRxiv}},
  doi = {10.1101/523712},
  abstract = {Transposable elements are abundant genetic components of eukaryotic genomes with important regulatory features affecting transcription, splicing, and recombination, among others. Here we demonstrate that the Murine Endogenous Retroviral Element (MuERV-L/MERVL) family of transposable elements drives the 3D reorganisation of the genome in the early mouse embryo. By generating Hi-C data in 2-cell-like cells, we show that MERLV elements promote the formation of insulating domain boundaries through-out the genome in vivo and in vitro. The formation of these boundaries is coupled to the upregulation of directional transcription from MERVL, which results in the activation of a subset of the gene expression programme of the 2-cell stage embryo. Domain boundaries in the 2-cell stage embryo are transient and can be remodelled without undergoing cell division. Remarkably, we find extensive inter-strain MERVL variation, suggesting multiple non-overlapping rounds of recent genome invasion and a high regulatory plasticity of genome organisation. Our results demonstrate that MERVL drive chromatin organisation during early embryonic development shedding light into how nuclear organisation emerges during zygotic genome activation in mammals.},
  chapter = {New Results},
  copyright = {\textcopyright{} 2019, Posted by Cold Spring Harbor Laboratory. This pre-print is available under a Creative Commons License (Attribution-NonCommercial 4.0 International), CC BY-NC 4.0, as described at http://creativecommons.org/licenses/by-nc/4.0/},
  langid = {english},
  file = {C\:\\Users\\Rilakkuma\\Zotero\\storage\\US7RZW6Y\\Kruse et al. - 2019 - Transposable elements drive reorganisation of 3D c.pdf;C\:\\Users\\Rilakkuma\\Zotero\\storage\\H379WATR\\523712v1.html}
}

@article{kubon_renb:PromoterproximalCTCF2021,
  title = {Promoter-Proximal {{CTCF}} Binding Promotes Distal Enhancer-Dependent Gene Activation},
  author = {Kubo, Naoki and Ishii, Haruhiko and Xiong, Xiong and Bianco, Simona and Meitinger, Franz and Hu, Rong and Hocker, James D. and Conte, Mattia and Gorkin, David and Yu, Miao and Li, Bin and Dixon, Jesse R. and Hu, Ming and Nicodemi, Mario and Zhao, Huimin and Ren, Bing},
  year = {2021},
  month = jan,
  journal = {Nature Structural \& Molecular Biology},
  pages = {1--10},
  publisher = {{Nature Publishing Group}},
  issn = {1545-9985},
  doi = {10.1038/s41594-020-00539-5},
  abstract = {The CCCTC-binding factor (CTCF) works together with the cohesin complex to drive the formation of chromatin loops and topologically associating domains, but its role in gene regulation has not been fully defined. Here, we investigated the effects of acute CTCF loss on chromatin architecture and transcriptional programs in mouse embryonic stem cells undergoing differentiation to neural precursor cells. We identified CTCF-dependent enhancer\textendash promoter contacts genome-wide and found that they disproportionately affect genes that are bound by CTCF at the promoter and are dependent on long-distance enhancers. Disruption of promoter-proximal CTCF binding reduced both long-range enhancer\textendash promoter contacts and transcription, which were restored by artificial tethering of CTCF to the promoter. Promoter-proximal CTCF binding is correlated with the transcription of over 2,000 genes across a diverse set of adult tissues. Taken together, the results of our study show that CTCF binding to promoters may promote long-distance enhancer-dependent transcription at specific genes in diverse cell types.},
  copyright = {2021 The Author(s), under exclusive licence to Springer Nature America, Inc.},
  langid = {english},
  file = {C\:\\Users\\Rilakkuma\\Zotero\\storage\\TMQ7LC2J\\Kubo et al. - 2021 - Promoter-proximal CTCF binding promotes distal enh.pdf;C\:\\Users\\Rilakkuma\\Zotero\\storage\\MX7HCWAR\\s41594-020-00539-5.html}
}

@article{kuhnej_geyerpk:TestInsulator2003,
