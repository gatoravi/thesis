@article{akbarios_drewellra:UnravelingCisregulatory2006,
  title = {Unraveling Cis-Regulatory Mechanisms at the Abdominal-{{A}} and {{Abdominal-B}} Genes in the {{Drosophila}} Bithorax Complex},
