@article{lok_smalest:GeneralityFunctional1996,
  title = {Generality of a Functional Initiator Consensus Sequence},
  author = {Lo, Kiersten and Smale, Stephen T.},
  year = {1996},
  month = dec,
  journal = {Gene},
  volume = {182},
  number = {1},
  pages = {13--22},
  issn = {0378-1119},
  doi = {10.1016/S0378-1119(96)00438-6},
  abstract = {Transcriptional initiator (Inr) elements, like TATA boxes, are found in the core promoters of many eukaryotic protein-coding genes. To facilitate the dissection of transcription initiation mechanisms, and to identify Inr elements within known and newly-discovered genes, the precise DNA sequence requirements for Inr activity must be defined. Previously, we reported a preliminary Inr consensus sequence based on an extensive mutant analysis carried out in HeLa cell extracts. This analysis was limited, however, because it was performed only with an in vitro transcription assay and with the mutants in only one promoter context. In this study, we have assessed the general validity of the functional Inr consensus sequence by analyzing a selected set of mutants in additional promoter contexts and with both in vivo and in vitro assays. In addition, we analyzed the evolutionary conservation of the Inr consensus sequence by determining the sequence requirements for Inr function in Drosophila embryo extracts. The results demonstrate the generality and strict conservation among vertebrates and invertebrates of the Inr consensus sequence, Py Py A+1 N T/A Py Py.},
  langid = {english},
  keywords = {Promoter,RNA polymerase II,Transcription},
  file = {C\:\\Users\\Rilakkuma\\Zotero\\storage\\IPWUVDQQ\\Lo and Smale - 1996 - Generality of a functional initiator consensus seq.pdf;C\:\\Users\\Rilakkuma\\Zotero\\storage\\HCKMCQ7X\\S0378111996004386.html}
}

@article{lopez-novoajm_nietoma:InflammationEMT2009,
  title = {Inflammation and {{EMT}}: An Alliance towards Organ Fibrosis and Cancer Progression},
  shorttitle = {Inflammation and {{EMT}}},
  author = {{L{\'o}pez-Novoa}, Jose Miguel and Nieto, M. Angela},
