@article{huseyinmk_kloserj:LivecellSingle2021,
  title = {Live-Cell Single Particle Tracking of {{PRC1}} Reveals a Highly Dynamic System with Low Target Site Occupancy},
  author = {Huseyin, Miles K. and Klose, Robert J.},
  year = {2021},
  month = feb,
  journal = {Nature Communications},
  volume = {12},
  number = {1},
  pages = {887},
  publisher = {{Nature Publishing Group}},
  issn = {2041-1723},
  doi = {10.1038/s41467-021-21130-6},
  abstract = {Polycomb repressive complex 1 (PRC1) is an essential chromatin-based repressor of gene transcription. How PRC1 engages with chromatin to identify its target genes and achieve gene repression remains poorly defined, representing a major hurdle to our understanding of Polycomb system function. Here, we use genome engineering and single particle tracking to dissect how PRC1 binds to chromatin in live mouse embryonic stem cells. We observe that PRC1 is highly dynamic, with only a small fraction stably interacting with chromatin. By integrating subunit-specific dynamics, chromatin binding, and abundance measurements, we discover that PRC1 exhibits low occupancy at target sites. Furthermore, we employ perturbation approaches to uncover how specific components of PRC1 define its kinetics and chromatin binding. Together, these discoveries provide a quantitative understanding of chromatin binding by PRC1 in live cells, suggesting that chromatin modification, as opposed to PRC1 complex occupancy, is central to gene repression.},
  copyright = {2021 The Author(s)},
  langid = {english},
  file = {C\:\\Users\\Rilakkuma\\Zotero\\storage\\HHDAFDPB\\Huseyin and Klose - 2021 - Live-cell single particle tracking of PRC1 reveals.pdf}
}

@article{imamurak_akimitsun:LongNoncoding2014,
  title = {Long Noncoding {{RNA NEAT1-dependent SFPQ}} Relocation from Promoter Region to Paraspeckle Mediates {{IL8}} Expression upon Immune Stimuli},
  author = {Imamura, Katsutoshi and Imamachi, Naoto and Akizuki, Gen and Kumakura, Michiko and Kawaguchi, Atsushi and Nagata, Kyosuke and Kato, Akihisa and Kawaguchi, Yasushi and Sato, Hiroki and Yoneda, Misako and Kai, Chieko and Yada, Tetsushi and Suzuki, Yutaka and Yamada, Toshimichi and Ozawa, Takeaki and Kaneki, Kiyomi and Inoue, Tsuyoshi and Kobayashi, Mika and Kodama, Tatsuhiko and Wada, Youichiro and Sekimizu, Kazuhisa and Akimitsu, Nobuyoshi},
  year = {2014},
  month = feb,
  journal = {Molecular Cell},
  volume = {53},
  number = {3},
  pages = {393--406},
  issn = {1097-4164},
  doi = {10.1016/j.molcel.2014.01.009},
  abstract = {Although thousands of long noncoding RNAs (lncRNAs) are localized in the nucleus, only a few dozen have been functionally characterized. Here we show that nuclear enriched abundant transcript 1 (NEAT1), an essential lncRNA for the formation of nuclear body paraspeckles, is induced by influenza virus and herpes simplex virus infection as well as by Toll-like receptor3-p38 pathway-triggered poly I:C stimulation, resulting in excess formation of paraspeckles. We found that NEAT1 facilitates the expression of antiviral genes including cytokines such as interleukin-8 (IL8). We found that splicing factor proline/glutamine-rich (SFPQ), a NEAT1-binding paraspeckle protein, is a repressor of IL8 transcription, and that NEAT1 induction relocates SFPQ from the IL8 promoter to the paraspeckles, leading to transcriptional activation of IL8. Together, our data show that NEAT1 plays an important role in the innate immune response through the transcriptional regulation of antiviral genes by the stimulus-responsive cooperative action of NEAT1 and SFPQ.},
  langid = {english},
  pmid = {24507715},
  keywords = {Gene Expression Regulation,HeLa Cells,Herpesvirus 1; Human,Humans,Immunity; Innate,Interleukin-8,Measles virus,Orthomyxoviridae,Promoter Regions; Genetic,Protein Transport,RNA-binding proteins,RNA; Long Noncoding,Transcription; Genetic}
}

@article{inadas_shimment:RegulationElongation2000,
